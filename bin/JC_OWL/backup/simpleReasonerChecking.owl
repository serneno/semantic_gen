    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#hasCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#hasCountry">
