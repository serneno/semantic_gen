        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#Region"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#Country"/>
        <rdfs:comment>This property entails what country a Region is within.
It is not an inverse of hasRegion since both Countries and Cases have regions.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#hasDay -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2020/CovidOntology/1.0.0#hasDay">
